    "lint:fix": "tslint --project tsconfig.json --fix 'src/**/*.ts'",
    "offline": "SLS_DEBUG=* serverless offline",
    "debug": "SET SLS_DEBUG=* && node --inspect /Users/ctsuser1/Documents/ArunWorkspace/TamilKurral-api/kurral-api/node_modules/@types/serverless offline -s dev"
